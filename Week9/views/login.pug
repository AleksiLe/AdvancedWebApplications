extends layout 

block content 
    p Welcome to login
    form(method='POST', id='login-form')
        p
            label Email:
            br
            input(type='email', name='email')
        p
            label Password:
            br
            input(type='password', name='password')
        p
            input(type='submit', value='Login')
        p(id="error")
    script(src="/javascripts/login.js") 